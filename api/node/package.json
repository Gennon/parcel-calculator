{
  "name": "parcel-calculator-api",
  "version": "0.1.0",
  "description": "The Api for the Parcel Calculator",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "watch-tsc": "tsc -w",
    "watch-deploy": "nodemon dist/index.js",
    "start": "concurrently \"npm run watch-tsc\" \"npm run watch-deploy\"",
    "build": "webpack"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/jest.setup.ts"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^4.12.0",
    "mongoose": "^6.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.5.0",
    "jest": "^29.3.1",
    "mongodb-memory-server": "^8.10.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
